
<div class='proyectos'>
<h1 class="titulos">
  PROYECTO NRO <%= proy.boletin %>
</h1>


<h3> <%= proy.resumen %> </h3>
<h3> Su estado actual es <%= proy.estado %> </h3>
<a class="link" href=" <%= proy.url %> "> Ver en pagina del senado </a>

<!-- Senadores Involucrados -->

<div style="align: center;">
  <div class="contenedor-cards">
  <% for (i = 0; i < senadores.length; i++) { %>

    <div class="card-senadores">
      <img src=" <%= fotos[i] %> " alt="foto" style="width:100%">
      <h1> <%= senadores[i].nombre %> </h1>
      <h1> <%= senadores[i].apellido_paterno %> </h1>
      <p class="title-senadores"> <%= senadores[i].partido_politico %> </p>
        <a class="link-senadores" href=" <%= senadores[i].url_twitter %> ">Twitter</a>
      <p><button>Ver Perfil</button></p>
    </div>

  <% } %>
  </div>
</div>

<!-- Timeline del Proyecto -->
<div>
  <div class="container">

     <div class="timeline-block timeline-block-right">
        <div class="marker"></div>
        <div class="timeline-content">
           <h3>Inicio</h3>
           <span> <%= fecha %> </span>
        </div>
     </div>

     <div class="timeline-block timeline-block-left">
        <div class="marker"></div>
        <div class="timeline-content">
           <h3>Estado actual</h3>
           <span><%= proy.estado %></span>
        </div>
     </div>

  </div>
</div>

</div>

