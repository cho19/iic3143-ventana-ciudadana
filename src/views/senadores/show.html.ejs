<div class="grid-container">
  <img id="img-senador" src=<%= senador.url_foto %> alt="Foto de senador">
  <h1 id="nombre-senador"> <%= senador.nombre + " " + senador.apellido_paterno + " " + senador.apellido_materno %> </h1>
  <div id="informacion-personal">
    <a href=<%= senador.url_curriculum%> target="_blank">Ver Curr√≠culum</a> ||
    <a href=<%= senador.url_twitter%> target="_blank"> Ver Twitter</a>
  </div>
  <h1 id="titulo-proyectos">Proyectos</h1>
  <ul id="lista-proyectos">
    <% proyectos.slice(-10).forEach((proyecto) => { %>
      <li class="item-proyecto"><a href=<%= proyecto.url%>><%=proyecto.resumen%></a></li>
    <% }) %>
  </ul>
</div>
