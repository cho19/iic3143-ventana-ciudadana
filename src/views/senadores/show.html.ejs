<div class="grid-container">
  <div class="grid-item"><img src=<%= senador.url_foto %> alt="Foto de senador"></div>
  <div class="grid-item" id="nombre-senador"> <h1> <%= senador.nombre + " " + senador.apellido_paterno + " " + senador.apellido_materno %> </h1> </div>
  <div class="grid-item"> <a href=<%= senador.url_curriculum%> target="_blank">Ver Curr√≠culum</a> </div>
  <div class="grid-item" id="lista-proyectos">
    <ul>
      <% proyectos.forEach((proyecto) => { %>
        <li><a href=<%= proyecto.url%>><%=proyecto.resumen%></a></li>
      <% }) %>
    </ul>
  </div>
  <div class="grid-item"></div>
</div>
